{
  "name": "laydesc-api",
  "version": "0.0.1",
  "description": "LayDesc API. Programming interface for creating LayDesc documents. LayDesc API allows generating a generic representation of LayDesc document content (in JSON format). To visualize a LayDesc document, use this API with a LayDesc-renderer (currently LayDesc-renderer-html or LayDesc-renderer-InDesign)",
  "main": "./dist/layDesc.js",
  "typings": "./dist/layDesc.d.ts",
  "scripts": {
    "build-typedoc": "typedoc --options ./typedoc.js ./src/",
    "clean-typedoc": "rm -rf ./lib/documentation"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:LayDesc/LayDesc-api.git"
  },
  "keywords": [
    "LayDesc",
    "layout-tools",
    "layout",
    "print",
    "graphic-design",
    "pdf",
    "pdf-document"
  ],
  "author": "nicolas baldran",
  "license": "MIT",
  "dependencies": {
    "npm": "^6.0.1",
    "to": "^0.2.9",
    "update": "^0.7.4"
  },
  "devDependencies": {
    "extract-loader": "^2.0.1",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "ts-loader": "^4.3.0",
    "tslint": "^5.10.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.3",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  }
}
